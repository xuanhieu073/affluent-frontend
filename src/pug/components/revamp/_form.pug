mixin form
  form(x-data="paramsHandler")
    .form-builder__input 
      label(for="firstname") firstname
      input#firstname(type="text", name="firstname", data-param-key="firstnames" data-csv-data-url="http://localhost:4999/file/form-params.css" data-default="default")
    .form-builder__input 
      label(for="checkboxlist")
      input#checkboxlist(type="checkbox", name="checkboxlist")
    .form-builder__input
      .dropdown(data-unselected-label='Please select...')
        span.a-dropdown
          select#interested_in.a-dropdown__select(name='interested_in', aria-required='true')
            option.dropdown__option(value='') Please select...
            | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
            option(value='SelectOption') Select Option
            option(value='s$10-toastbox-campaign') S$10 Toastbox Campaign
            option(value='redemption-on-singlife-joyful-bundle') Redemption on Singlife Joyful Bundle
            option(value='launch-on-tranche-series-singlife-secure-saver') Launch on tranche series- Singlife Secure Saver
            option(value='claims-enquiry') Claims enquiry
            option(value='mindef-and-mha-group-policy') MINDEF&amp; MHA Group Policy
            option(value='others') Others
          span.a-dropdown__ui
        .select-selected
          .selected-option S$10 Toastbox Campaign
          |  
          div
            svg(width='16', height='8', viewBox='0 0 16 8', fill='none', xmlns='http://www.w3.org/2000/svg')
              path(d='M14.6663 0.5L7.99967 7.16667L1.33301 0.5', stroke-linecap='round')
        .select-items(style='display: none;')
          div(data-index='1') Select Option
          .selected(data-index='2') S$10 Toastbox Campaign
          div(data-index='3') Redemption on Singlife Joyful Bundle
          div(data-index='4') Launch on tranche series- Singlife Secure Saver
          div(data-index='5') Claims enquiry
          div(data-index='6') MINDEF&amp; MHA Group Policy
          div(data-index='7') Others
    .form-builder__input 
      textarea#textarea(name="textarea")